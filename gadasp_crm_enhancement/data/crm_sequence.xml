<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <!-- Sequences for sale.order -->
        <record id="seq_type_crm_lead" model="ir.sequence.type">
            <field name="name">CRM Lead</field>
            <field name="code">crm.lead</field>
        </record>

        <record id="seq_ticket_crm_lead" model="ir.sequence">
            <field name="name">CRM Lead</field>
            <field name="code">crm.lead</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
        
        <record id="seq_post_crm_lead" model="ir.sequence">
            <field name="name">Oficio GADSAP</field>
            <field name="code">crm.lead</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field eval="1" name="number_next"/>
            <field name="suffix">-%(year)s GADPRSAP</field>
            <field eval="1" name="number_increment"/>
        </record>

    </data>
</openerp>
